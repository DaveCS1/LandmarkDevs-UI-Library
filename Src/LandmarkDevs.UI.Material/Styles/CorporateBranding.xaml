<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:LandmarkDevs.UI.Material.Converters"
                    xmlns:properties="clr-namespace:LandmarkDevs.UI.Material.Properties">

    <converters:BitmapToImageSourceConverter x:Key="BitmapConverter" />

    <!--#region Yokogawa Colors-->
    <!--#region CI Colors-->
    <Color x:Key="YokogawaYellow">#FFEE00</Color>
    <Color x:Key="YokogawaBlue">#004F9B</Color>
    <Color x:Key="YokogawaGreen">#00B04D</Color>
    <SolidColorBrush x:Key="YokogawaYellowBrush" Color="{StaticResource YokogawaYellow}" />
    <SolidColorBrush x:Key="YokogawaBlueBrush" Color="{StaticResource YokogawaBlue}" />
    <SolidColorBrush x:Key="YokogawaGreenBrush" Color="{StaticResource YokogawaGreen}" />
    <!--#endregion-->
    <!--#region Corporate Communications Colors-->
    <!--#region 100%-->
    <Color x:Key="CommBlue100">#00316C</Color>
    <Color x:Key="CommYellow100">#f1bc1a</Color>
    <Color x:Key="CommGreen100">#00694c</Color>
    <Color x:Key="CommOrange100">#CE4E21</Color>
    <Color x:Key="CommGrey100">#5A707B</Color>
    <SolidColorBrush x:Key="CommBlue100Brush" Color="{StaticResource CommBlue100}" />
    <SolidColorBrush x:Key="CommYellow100Brush" Color="{StaticResource CommYellow100}" />
    <SolidColorBrush x:Key="CommGreen100Brush" Color="{StaticResource CommGreen100}" />
    <SolidColorBrush x:Key="CommOrange100Brush" Color="{StaticResource CommOrange100}" />
    <SolidColorBrush x:Key="CommGrey100Brush" Color="{StaticResource CommGrey100}" />
    <!--#endregion-->
    <!--#region 80%-->
    <Color x:Key="CommBlue80">#335A8A</Color>
    <Color x:Key="CommYellow80">#F5C956</Color>
    <Color x:Key="CommGreen80">#007E65</Color>
    <Color x:Key="CommOrange80">#D77243</Color>
